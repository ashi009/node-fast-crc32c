{
  "name": "fast-crc32c",
  "description": "CRC32C algorithm with hardware acceleration and software fallback.",
  "version": "0.1.1",
  "author": "Xiaoyi Shi <ashi009@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git://github.com/ashi009/node-fast-crc32c.git"
  },
  "main": "./loader",
  "engines": {
    "node": ">= 0.8.0"
  },
  "optionalDependencies": {
    "sse4_crc32": "^2.0.0"
  },
  "scripts": {
    "test": "mocha",
    "posttest": "mocha -R travis-cov",
    "coverage": "mocha -R html-cov > coverage.html",
    "benchmark": "node benchmark"
  },
  "keywords": [
    "crc",
    "crc32",
    "crc32c",
    "sse",
    "sse4",
    "sse4.2"
  ],
  "devDependencies": {
    "benchtable": "*",
    "blanket": "*",
    "mocha": "*",
    "should": "*",
    "travis-cov": "*"
  },
  "config": {
    "blanket": {
      "pattern": [
        "crc32c.js",
        "loader.js"
      ]
    },
    "travis-cov": {
      "threshold": 90
    }
  }
}
